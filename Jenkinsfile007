pipeline {
    agent any
    stages {
        stage('SCM') {
            steps {
                git url: 'https://github.com/Dasarivenkatesh/myweb.git'
            }
        }
        stage('build && SonarQube analysis') {
            steps {
                withSonarQubeEnv('My SonarQube Server') {
                   withMaven(maven:'Maven 3.5') {
                        sh 'mvn clean package sonar:sonar'
                    }
                }
            }
        }
   }
}

